{
  "name": "paint-optimizer",
  "version": "1.0.0",
  "description": "ES2015+ Babel, Webpack and React project",
  "scripts": {
    "build": "webpack --progress --colors",
    "start": "webpack-dev-server --progress --colors",
    "test": "mocha --compilers js:babel/register app/**/*-test.js --recursive",
    "test:w": "chokidar '**/*.js' -i 'node_modules' -c 'npm run --silent test'",
    "test:d": "mocha --compilers js:babel/register app/**/*-test.js --recursive --debug-brk"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/briandipalma/paint-optimizer.git"
  },
  "keywords": [
    "webpack",
    "React",
    "ES2015",
    "Babel"
  ],
  "author": "Brian Di Palma",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/briandipalma/paint-optimizer/issues"
  },
  "homepage": "https://github.com/briandipalma/paint-optimizer#readme",
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-core": "^5.6.15",
    "babel-loader": "^5.2.2",
    "chokidar-cli": "^1.0.0",
    "css-loader": "^0.15.1",
    "eslint": "^0.24.0",
    "eslint-plugin-react": "^2.6.4",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "style-loader": "^0.12.3",
    "webpack": "^1.10.0",
    "webpack-dev-server": "^1.10.1"
  },
  "dependencies": {
    "baobab": "^1.1.1",
    "baobab-react": "^0.1.1",
    "react": "^0.13.3"
  }
}
